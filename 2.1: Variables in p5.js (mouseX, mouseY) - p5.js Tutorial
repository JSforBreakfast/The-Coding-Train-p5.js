function setup() {
  createCanvas(600, 400);
  //background
  background(250, 200, 100);
  print("Hello Universe");
}

function draw() {
 
  //pink ellipse
  fill(250, 200, 200);
  ellipse(100, 100, 100, 100);
  //rectangle
 // strokeWeight(1);
  //fill(250, 100, 300);
  //rect(mouseX, mouseY, 180, 120, 120);

  //yellow ellipse
  noStroke();
  fill(5, 25, 150, 50);
  ellipse(mouseX, mouseY, 25, 25, 25, 25);

}

function mousePressed() {
background (250,250, 100);
}
